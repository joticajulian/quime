{
  "name": "quime",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build": "cd packages/frontend && yarn run build",
    "install": "yarn run install:frontend && yarn run install:backend",
    "install:frontend": "cd packages/frontend && yarn install",
    "install:backend": "cd packages/backend && yarn install",
    "start": "cd packages/backend && node src/start.js",
    "test": "jest --coverage --runInBand"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0"
  }
}
